{
  "name": "rerover",
  "env": "docker",
  "region": "oregon",
  "buildCommand": "cd ReRover && mvn clean package -DskipTests",
  "dockerfile": "ReRover/Dockerfile",
  "envVars": [
    {
      "key": "SPRING_PROFILES_ACTIVE",
      "value": "production"
    },
    {
      "key": "PORT",
      "value": "8080"
    }
  ],
  "autoDeploy": true
}
